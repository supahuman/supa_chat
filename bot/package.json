{
  "name": "supa_chatbot-backend",
  "version": "1.0.0",
  "description": "Backend server for supa_chatbot (inside bot directory)",
  "private": true,
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --watch . app.js",
    "chunking:status": "node scripts/chunking-control.js status",
    "chunking:list": "node scripts/chunking-control.js list",
    "chunking:switch": "node scripts/chunking-control.js switch",
    "chunking:test": "node scripts/chunking-control.js test",
    "strategy:status": "node scripts/chunking-strategy.js status",
    "strategy:switch": "node scripts/chunking-strategy.js switch",
    "strategy:clean": "node scripts/chunking-strategy.js clean",
    "strategy:reload": "node scripts/chunking-strategy.js reload",
    "strategy:validate": "node scripts/chunking-strategy.js validate",
    "strategy:config": "node scripts/chunking-strategy.js config"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@huggingface/inference": "^4.11.0",
    "@langchain/cohere": "^0.3.4",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.58",
    "@langchain/openai": "^0.1.0",
    "@langchain/textsplitters": "^0.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "groq-sdk": "^0.5.0",
    "jsdom": "^24.0.0",
    "langchain": "^0.3.34",
    "mammoth": "^1.6.0",
    "mongoose": "^8.6.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.56.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "optionalDependencies": {
    "@browserbasehq/stagehand": "^1.14.0",
    "@ibm-cloud/watsonx-ai": "^1.6.13",
    "ibm-cloud-sdk-core": "^5.4.3"
  }
}
